NODE_ENV=development
PORT=3001
API_BASE_URL=https://capifit.app.br:3001
FRONTEND_URL=https://capifit.app.br:5173
DATABASE_URL=postgresql://capifit_user:__ASK_AT_RUNTIME__@localhost:5432/capifit_db?schema=public
SHADOW_DATABASE_URL=postgresql://capifit_user:__ASK_AT_RUNTIME__@localhost:5432/capifit_shadow?schema=public
JWT_ACCESS_SECRET=__SET_ME__
JWT_REFRESH_SECRET=__SET_ME__
JWT_ACCESS_TTL=900
JWT_REFRESH_TTL=1209600
PASSWORD_SALT_ROUNDS=12
REDIS_URL=redis://localhost:6379
SMTP_HOST=smtp.mailgun.org
SMTP_PORT=587
SMTP_USER=capifit
SMTP_PASS=__SET_ME__
SMTP_FROM="CapiFit <no-reply@capifit.com>"
ENABLE_EMAIL_NOTIFICATIONS=true
NOTIFICATION_WORKER_CONCURRENCY=3
FILE_STORAGE_DRIVER=local
FILE_STORAGE_LOCAL_PATH=./storage
FILE_STORAGE_S3_BUCKET=capifit-files
FILE_STORAGE_S3_REGION=us-east-1
FILE_STORAGE_S3_ENDPOINT=
FILE_STORAGE_S3_ACCESS_KEY=
FILE_STORAGE_S3_SECRET_KEY=
LOG_LEVEL=info
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
WEBSOCKET_PATH=/socket.io
WEBSOCKET_ALLOWED_ORIGINS=http://localhost:5173
